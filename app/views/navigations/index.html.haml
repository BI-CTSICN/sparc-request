.grid_3
  = render :partial => 'catalog/service_search'
  = render :partial => 'catalog/service_accordion', :locals => {:institutions => @institutions}
.grid_6
  #welcome_message= render :partial => 'catalog/description', :locals => {:organization => @organization}
.grid_3.catalog-view-right.right
  #services.ui-widget.ui-widget-content.cart-view
    %h3
      My Services
      %span#cart-help Help
    .line-items= render :partial => 'catalog/cart', :locals => {:service_request => @service_request}
  .submit-request-button
    = image_tag 'submit_request.png'
  .dashboard-button= link_to image_tag('dashboard.png'), '#'
