%h3 Core Approvals
%table{:id => 'core_approvals'}
  %tr
    %th Nursing/Nutrition Approved
    %td= check_box_tag "sub_service_request_nursing_nutrition_approved", true, @sub_service_request.nursing_nutrition_approved, :'data-sub_service_request_id' => @sub_service_request.id, :class => 'fulfillment_data', :disabled => @sub_service_request.nursing_nutrition_approved?
    %th Lab Approved
    %td= check_box_tag "sub_service_request_lab_approved", true, @sub_service_request.lab_approved, :'data-sub_service_request_id' => @sub_service_request.id, :class => 'fulfillment_data', :disabled => @sub_service_request.lab_approved?
    %th Imaging Approved
    %td= check_box_tag "sub_service_request_imaging_approved", true, @sub_service_request.imaging_approved, :'data-sub_service_request_id' => @sub_service_request.id, :class => 'fulfillment_data', :disabled => @sub_service_request.imaging_approved?
